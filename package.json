{
  "name": "@lecualab/test-api",
  "description": "Test API",
  "author": {
    "name": "Crist√≥bal Gajardo Vera",
    "email": "cristobalgajardo.v@gmail.com"
  },
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "prebuild": "rimraf app",
    "build": "nest build",
    "format": "prettier --check --ignore-unknown --write",
    "format:all": "npm run format -- .",
    "lint": "eslint",
    "lint:all": "npm run lint -- \"src/**/*.ts\"",
    "lint:types": "tsc --noEmit",
    "prestart:offline": "npm run build && cp serverless/serverless.local.yaml serverless.yaml",
    "start:offline": "serverless offline --stage local start --reloadHandler",
    "poststart:offline": "rm serverless.yaml",
    "start": "nest start",
    "start:debug": "npm run start:dev -- --debug",
    "start:dev": "npm run start -- --watch",
    "start:docker": "docker compose -f compose.yml -f compose.dev.yml up --build -d",
    "stop:docker": "docker compose -f compose.yml -f compose.dev.yml down",
    "test": "jest --verbose",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.16.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/swagger": "^11.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "5",
    "helmet": "^8.0.0",
    "nest-typed-config": "^2.9.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@faker-js/faker": "^9.5.0",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/cli": "^11.0.3",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.jest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@types/aws-lambda": "^8.10.147",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-extended": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "~4.1.0",
    "rimraf": "^6.0.1",
    "serverless": "^4.6.3",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-log-retention": "2.1.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
